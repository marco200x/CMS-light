<h1>Listado de Usuarios</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Fecha Nacimiento</th>
    <th>Login</th>
    <th>Pass</th>
    <th>Tipo</th>
  </tr>

<% @usuarios.each do |usuario| %>
  <tr>
    <td><%=h usuario.nombre %></td>
    <td><%=h usuario.apellido %></td>
    <td><%=h usuario.fecha_nacimiento %></td>
    <td><%=h usuario.login %></td>
    <td><%=h usuario.pass %></td>
    <td><%=h usuario.tipo %></td>
    <td><%= link_to 'Mostrar', usuario %></td>
    <td><%= link_to 'Editar', edit_usuario_path(usuario) %></td>
    <td><%= link_to 'Eliminar', usuario, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo Usuario', new_usuario_path %>
<%= link_to 'Regresa al MENU', configuracions_path %>